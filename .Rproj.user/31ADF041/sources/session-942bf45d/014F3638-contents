categories <- extract_categories(year_2018)
ciclo_educativo <- extract_ciclo_educativo(year_2018)
barrios <- extract_barrios(year_2018)
values <- extract_values(year_2018)

expand.grid(
  barrios = barrios,
  ciclo_educativo = ciclo_educativo,
  categories = categories
)


as.numeric(unlist(as.vector(values)))

values


read_and_clean(year_2018, 2018)
